page{
  background-color: rgb(244, 244, 244);
}
text {
  white-space: nowrap;
}
.text-ellipse {
  text-overflow: ellipsis;
  overflow: hidden;
}
.bgWhite {
  background-color: #fff;
}
.warpper {
  width: 100%;
  margin: 20rpx 20rpx;
  background-color: #fff;
  border:2rpx solid rgba(0,0,0,0.1);
  border-radius:4px;
}
.flex1{
  flex:1;
}
.flex9{
  flex:9;
}
.input {
  width: 120rpx;
  font-family: San Francisco Display;
	font-size: 32rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 244rpx;
	letter-spacing: 0rpx;
	color: #000;
}
.fix-bottom {
  width: 100%;
  height: 80rpx;
  position: fixed;
  bottom: 0;
}
.bottom-wrapper {
  border-top:2rpx solid rgba(0,0,0,.1);
}
.icon {
  display: inline-block;
  width: 60rpx;
  height: 60rpx;
  background-size: cover;
  background-position: center;
}
.warn {
  width: 8rpx;
  height: 40rpx;
}
.section-body {
  padding: 20rpx 0;
}